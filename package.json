{
  "name": "mywebsite2",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "build": "vite build",
    "build:vercel": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx}\"",
    "docs:lint": "markdownlint docs/ *.md --ignore .vercelignore",
    "docs:lint:fix": "markdownlint docs/ *.md --fix --ignore .vercelignore",
    "quality": "pnpm lint && pnpm docs:lint && pnpm format:check && pnpm type-check",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:coverage:ui": "vitest --ui --coverage",
    "prepare": "husky install",
    "lint-staged": "lint-staged",
    "type-check": "tsc -b --noEmit"
  },
  "dependencies": {
    "@floating-ui/react": "^0.27.15",
    "@headlessui/react": "^2.2.9",
    "@heroicons/react": "^2.2.0",
    "@hookform/resolvers": "^5.2.2",
    "@tailwindcss/vite": "^4.1.18",
    "@tanstack/react-query": "^5.90.12",
    "@tanstack/react-query-devtools": "^5.91.1",
    "axios": "^1.13.2",
    "dompurify": "^3.3.1",
    "framer-motion": "^12.23.26",
    "highlight.js": "^11.11.1",
    "i18next": "^24.2.3",
    "i18next-browser-languagedetector": "^8.2.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-google-recaptcha-v3": "^1.11.0",
    "react-hook-form": "^7.69.0",
    "react-i18next": "^15.7.4",
    "react-markdown": "^10.1.0",
    "react-router": "^7.11.0",
    "rehype-highlight": "^7.0.2",
    "remark-gfm": "^4.0.1",
    "tailwindcss": "^4.1.18",
    "zod": "^4.2.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.2.0",
    "@commitlint/config-conventional": "^20.2.0",
    "@eslint/js": "^9.39.2",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@types/node": "^24.3.0",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@typescript-eslint/eslint-plugin": "^8.50.1",
    "@typescript-eslint/parser": "^8.50.1",
    "@vitejs/plugin-react-swc": "^3.11.0",
    "@vitest/coverage-v8": "^4.0.16",
    "@vitest/eslint-plugin": "^1.6.4",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react-dom": "^1.53.1",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.26",
    "eslint-plugin-react-x": "^1.53.1",
    "eslint-plugin-testing-library": "^7.15.3",
    "eslint-plugin-unused-imports": "^4.3.0",
    "globals": "^15.15.0",
    "husky": "^9.1.7",
    "jsdom": "^26.1.0",
    "lint-staged": "^15.5.2",
    "markdownlint-cli": "^0.47.0",
    "prettier": "^3.7.4",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.50.1",
    "vite": "^7.3.0",
    "vitest": "^4.0.16"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --write",
      "eslint --fix",
      "vitest related --run"
    ],
    "*.md": [
      "markdownlint --fix"
    ]
  },
  "pnpm": {
    "ignoredBuiltDependencies": [
      "@swc/core",
      "esbuild"
    ],
    "onlyBuiltDependencies": [
      "@tailwindcss/oxide",
      "@vercel/speed-insights"
    ],
    "overrides": {
      "mdast-util-to-hast": ">=13.2.1"
    }
  }
}
