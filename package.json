{
  "name": "mywebsite2",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "build": "vite build",
    "build:vercel": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx}\"",
    "docs:lint": "markdownlint docs/ *.md --ignore .vercelignore",
    "docs:lint:fix": "markdownlint docs/ *.md --fix --ignore .vercelignore",
    "quality": "pnpm lint && pnpm docs:lint && pnpm format:check && pnpm type-check",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "prepare": "husky install",
    "lint-staged": "lint-staged",
    "type-check": "tsc -b --noEmit"
  },
  "dependencies": {
    "@floating-ui/react": "^0.27.15",
    "@headlessui/react": "^2.2.7",
    "@heroicons/react": "^2.2.0",
    "@hookform/resolvers": "^5.2.1",
    "@tailwindcss/vite": "^4.1.12",
    "@tanstack/react-query": "^5.85.3",
    "@tanstack/react-query-devtools": "^5.85.3",
    "@vercel/analytics": "^1.5.0",
    "@vercel/speed-insights": "^1.2.0",
    "axios": "^1.13.2",
    "dompurify": "^3.2.6",
    "framer-motion": "^12.23.12",
    "highlight.js": "^11.11.1",
    "i18next": "^24.2.3",
    "i18next-browser-languagedetector": "^8.2.0",
    "react": "^19.2.1",
    "react-dom": "^19.2.1",
    "react-google-recaptcha-v3": "^1.11.0",
    "react-hook-form": "^7.62.0",
    "react-i18next": "^15.6.1",
    "react-markdown": "^10.1.0",
    "react-router": "^7.8.1",
    "rehype-highlight": "^7.0.2",
    "remark-gfm": "^4.0.1",
    "tailwindcss": "^4.1.12",
    "zod": "^4.0.17"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.2.0",
    "@commitlint/config-conventional": "^20.2.0",
    "@eslint/js": "^9.33.0",
    "@testing-library/jest-dom": "^6.7.0",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^24.3.0",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "@typescript-eslint/eslint-plugin": "^8.39.1",
    "@typescript-eslint/parser": "^8.39.1",
    "@vitejs/plugin-react-swc": "^3.11.0",
    "@vitest/eslint-plugin": "^1.3.4",
    "eslint": "^9.33.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react-dom": "^1.52.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "eslint-plugin-react-x": "^1.52.5",
    "eslint-plugin-testing-library": "^7.6.6",
    "eslint-plugin-unused-imports": "^4.1.4",
    "globals": "^15.15.0",
    "husky": "^8.0.3",
    "jsdom": "^26.1.0",
    "lint-staged": "^15.5.2",
    "markdownlint-cli": "^0.47.0",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "typescript": "~5.7.3",
    "typescript-eslint": "^8.39.1",
    "vite": "^6.4.1",
    "vitest": "^3.2.4"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --write",
      "eslint --fix",
      "vitest related --run"
    ],
    "*.md": [
      "markdownlint --fix"
    ]
  },
  "pnpm": {
    "ignoredBuiltDependencies": [
      "@swc/core",
      "esbuild"
    ],
    "onlyBuiltDependencies": [
      "@tailwindcss/oxide",
      "@vercel/speed-insights"
    ],
    "overrides": {
      "mdast-util-to-hast": ">=13.2.1"
    }
  }
}
